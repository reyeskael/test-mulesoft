      <types:shape format="weave" example="examples/sampleProfile.json"><![CDATA[%dw 2.0

type profileMetaData = {|
  username: String, 
  password: String
|} {"example": "{\\\"username\\\":\\\"michaelreyes00007\\\",\\\"password\\\":\\\"Test1234\\\"}"}

]]></types:shape>
